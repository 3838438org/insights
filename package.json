{
  "name": "insights-build",
  "description": "",
  "version": "0.0.14",
  "homepage": "",
  "main": "server",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Marius Andra",
    "email": "marius.andra@gmail.com"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "server"
  },
  "engines": {
    "node": ">= 7.6.0",
    "yarn": ">= 0.18.0"
  },
  "scripts": {
    "test": "npm run eslint && npm run mocha",
    "lint": "eslint packages --ignore-pattern=node_modules",
    "lint:fix": "eslint packages --fix --ignore-pattern=node_modules",
    "start": "NODE_ENV=development node --inspect packages/server/app",
    "build": "NODE_ENV=production packages/core/node_modules/.bin/webpack --profile --config packages/core/client/webpack.config.js",
    "analyze": "NODE_ENV=production ANALYZE_BUNDLE=true packages/core/node_modules/.bin/webpack --config packages/core/client/webpack.config.js",
    "production": "NODE_ENV=production node packages/server/app",
    "mocha": "mocha test/ --recursive",
    "prepublish": "npm run build"
  },
  "files": [
    "bin",
    "README.md"
  ],
  "dependencies": {
    "insights": "file:./packages/cli",
    "insights-core": "file:./packages/core",
    "insights-desktop": "file:./packages/desktop",
    "insights-server": "file:./packages/server"
  },
  "peerDependencies": {
    "webpack": "*"
  }
}
